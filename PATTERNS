DIAMOND:-

#include<stdio.h>
int main()
{
    int n,i,j,space;
    printf("Enter the number of rows for half of the diamond:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for (space=1;space<=n-i;space++)
        {
            printf(" ");
        }
        for(j=1;j<=(2*i-1);j++){
            printf("*");
        }
        printf("\n");
    }
    for(i=n-1;i>=1;i--){
        for(space=1;space<=n-i;space++)
        {
            printf(" ");
        }
        for(j=1;j<=(2*i-1);j++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


OUTPUT:-
Enter the number of rows for half of the diamond:5
    *
   ***
  *****
 *******
*********
 *******
  *****
   ***
    *
=== Code Execution Successful ===
HALLOW DIAMOND:-
#include<stdio.h>
int main()
{
    int n,i,j,space;
    printf("Enter the number of rows for half of the hallow diamond:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for (space=1;space<=n-i;space++)
        {
            printf(" ");
        }
        for(j=1;j<=(2*i-1);j++){
            if(j==1||j==(2*i-1))
            printf("*");
            else
            printf(" ");
        }
        printf("\n");
    }
    for(i=n-1;i>=1;i--){
        for(space=1;space<=n-i;space++)
        {
            printf(" ");
        }
        for(j=1;j<=(2*i-1);j++){
            if(j==1||j==(2*i-1))
            printf("*");
            else
            printf(" ");
        }
        printf("\n");
    }
    return 0;
}

OUTPUT:-
Enter the number of rows for half of the hallow diamond:5
    *
   * *
  *   *
 *     *
*       *
 *     *
  *   *
   * *
    *


=== Code Execution Successful ===

SQUARE:-
#include<stdio.h>
int main()
{
    int n;
    printf("Enter the number of rows of the square:");
    scanf("%d",&n);
    for (int i=0;i<n;i++)
    {
        for (int j=0;j<n;j++)
        {
            printf("*");
        }
        printf("\n");
    }
   
    return 0;
}
OUTPUT:-
Enter the number of rows of the square:5
*****
*****
*****
*****
*****


=== Code Execution Successful ===
