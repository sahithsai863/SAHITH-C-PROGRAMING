PERSON DETAILS:-

#include <stdio.h>
#include <string.h>

struct Person 
{
  char name[50];
  int citNo;
  float salary;
} p1;

int main() {

  strcpy(p1.name, "Raj Kumar");

  p1.citNo = 1984;
  p1. salary = 2500;

  printf("Name: %s\n", p1.name);
  printf("Citizenship No.: %d\n", p1.citNo);
  printf("Salary: %.2f", p1.salary);

  return 0;
}
OUTPUT:-
Name: Raj Kumar
Citizenship No.: 1984
Salary: 2500.00

=== Code Execution Successful ===
***************************************************************************

BOOK DETAILS:-

#include <stdio.h>
#include <string.h>

struct Book
{
  char bookname[50];
  char authorname[50];
  int pageNo;
  float price;
} p1,p2;

int main() {

  strcpy(p1.bookname, "c-programming");
  strcpy(p2.authorname,"Dennis Ritche");

  p1.pageNo = 19;
  p1.price = 250;

  printf("Book Name: %s\n", p1.bookname);
  printf("Author Name:%s\n",p2.authorname);
  printf("Page No: %d\n", p1.pageNo);
  printf("Price: %.2f", p1.price);

  return 0;
}
output:-
Book Name: c-programming
Author Name:Dennis Ritche
Page No: 19
Price: 250.00

=== Code Execution Successful ===
***********************************************************************

FLIGHT RESERVATION:-

#include <stdio.h>
#include <string.h>

struct flight
{
  char passengername[50];
  char destinationcity[50];
  char departurecity[50];
  int date;
} C1;

int main() {

  strcpy(C1.passengername, "Alice");
  strcpy(C1.destinationcity,"Newyork");
 strcpy(C1.departurecity,"Losangeles");
  C1.date = 20072024;
 

  printf("PASSENGER NAME: %s\n", C1.passengername);
  printf("DESTINATION CITY:%s\n",C1.destinationcity);
  printf("DEPARTURE CITY: %s\n", C1.departurecity);
  printf("DATE OF TRAVEL: %d", C1.date);

  return 0;
}

PASSENGER NAME: Alice
DESTINATION CITY:Newyork
DEPARTURE CITY: Losangeles
DATE OF TRAVEL: 20072024

=== Code Execution Successful ===
