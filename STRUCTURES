#include <stdio.h>
#include <string.h>

struct Person 
{
  char name[50];
  int citNo;
  float salary;
} p1;

int main() {

  strcpy(p1.name, "Raj Kumar");

  p1.citNo = 1984;
  p1. salary = 2500;

  printf("Name: %s\n", p1.name);
  printf("Citizenship No.: %d\n", p1.citNo);
  printf("Salary: %.2f", p1.salary);

  return 0;
}