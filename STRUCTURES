#include <stdio.h>
#include <string.h>

struct Person 
{
  char name[50];
  int citNo;
  float salary;
} p1;

int main() {

  strcpy(p1.name, "Raj Kumar");

  p1.citNo = 1984;
  p1. salary = 2500;

  printf("Name: %s\n", p1.name);
  printf("Citizenship No.: %d\n", p1.citNo);
  printf("Salary: %.2f", p1.salary);

  return 0;
}

book details:-

#include <stdio.h>
#include <string.h>

struct Book
{
  char bookname[50];
  char authorname[50];
  int pageNo;
  float price;
} p1,p2;

int main() {

  strcpy(p1.bookname, "c-programming");
  strcpy(p2.authorname,"Dennis Ritche");

  p1.pageNo = 19;
  p1.price = 250;

  printf("Book Name: %s\n", p1.bookname);
  printf("Author Name:%s\n",p2.authorname);
  printf("Page No: %d\n", p1.pageNo);
  printf("Price: %.2f", p1.price);

  return 0;
}
output:-
Book Name: c-programming
Author Name:Dennis Ritche
Page No: 19
Price: 250.00

