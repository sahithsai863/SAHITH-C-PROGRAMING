#include <stdio.h>
int main() 
{
  int A[2][2],B[2][2],sum[2][2];
  int i,j;
  for(i=0;i<2;i++)
  {
      for(j=0;j<2;j++)
  {
      scanf("%d",&A[i][j]);
  } 
}
  
  for(i=0;i<2;i++)
  {
      for(j=0;j<2;j++)
  {
      scanf("%d",&B[i][j]);
  }
}
for(i=0;i<2;i++)
{
    for(j=0;j<2;j++)
    { printf("%d",A[i][j]);
    }
    printf("\n");
}
for(i=0;i<2;i++)
{
    for(j=0;j<2;j++)
    { printf("%d",B[i][j]);
    }
    printf("\n");
}
    

sum[i][j]=A[i][j]+B[i][j];
{
    printf("A+B=%d",sum[i][j]);
}
printf("\n");
return 0;
}
