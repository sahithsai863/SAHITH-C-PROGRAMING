//WORKING OF ASSIGNMENT OPERATOR
#include <stdio.h>
int main ()
{
    int a=5,b=8,c=10;
    printf("a=a+b: %d\n",a+=b);
    printf("a=a+c: %d\n",a-=c);
    printf("a=a-b: %d\n",a-=b);
    printf("a=a-c: %d\n",a-=c);
    printf("a=a*b: %d\n",a*=b);
    printf("a=a*c: %d\n",a*=c);
    printf("a=a/b: %d\n",a/=b);
    printf("a=a/c: %d\n",a/=c);
    printf("a modulodivided by b: %d\n",a%=b);
    printf("a modulodivided by c: %d\n",a%=c);
    return 0;
}

OUTPUT:-

a=a+b: 13
a=a+c: 3
a=a-b: -5
a=a-c: -15
a=a*b: -120
a=a*c: -1200
a=a/b: -150
a=a/c: -15
a modulo divided by b: -7
a modulo divided by c: -7

//WORKING OF REGIONAL OPERATORS
//WORKING OF LOGICAL OPERATORS

LOGICAL AND:

#include <stdio.h>

int main() {
    int a = 5, b = 10, c = 15;
    if (a < b && b == c) {
        printf("Both conditions are true.\n");
    } else {
        printf("At least one condition is false.\n");
    }
    return 0;
}

OUTPUT:-
At least one condition is false.



